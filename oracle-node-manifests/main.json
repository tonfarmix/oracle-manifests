{
  "cronExpression": "*/30 * * * * *",
  "priceAggregator": "median",
  "sourceTimeout": 50000,
  "minValidSourcesPercentage": 10,
  "deviationCheck": {
    "deviationWithRecentValues": {
      "maxPercent": 35,
      "maxDelayMilliseconds": 300000
    }
  },
  "base": "USD",
  "defaultSource": ["tonconsole"],
  "tokens": {
    "TON": {
      "sources": [
        "tonconsole"
      ]
    },
    "USDT": {
      "source": [
        "tonconsole"
      ]
    },
    "STON": {
      "sources": [
        "tonconsole",
        "stonfi"
      ]
    },
    "SCALE": {
      "sources": [
        "tonconsole",
        "dedust"
      ]
    },
    "NOT": {
      "sources": [
        "tonconsole",
        "stonfi",
        "dedust"
      ]
    }
  }
}